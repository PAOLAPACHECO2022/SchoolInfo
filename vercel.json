{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Client/build"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/client/build/$1" }
    ]
  }
  